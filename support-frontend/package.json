    "lint-staged": "^15.2.0",
    "madge": "^6.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.3",
    "postcss": "^8.4.38",
    "postcss-loader": "^7.3.0",
    "postcss-pxtorem": "^6.0.0",
    "preact": "^10.4.6",
    "preact-render-to-string": "^5.2.6",
    "prettier": "^2.8.8",
    "prettier-eslint": "^16.3.0",
    "pretty-format": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-svg-loader": "^3.0.3",
