{
  "name": "support-frontend",
  "version": "1.0.0",
  "description": "Frontend for the supporter platform.",
  "scripts": {
    "clean": "echo 'Cleaning Public Folder' && rimraf public/compiled-assets",
    "validate": "echo 'Validating JS' && npm-run-all 'lint:fix' && yarn orphans",
    "lint:nibble": "eslint-nibble ./assets --ext .ts,.tsx",
    "lint:check": "eslint ./assets --ext .ts --ext .tsx",
    "lint:fix": "yarn lint:check --fix",
    "build-dev": "webpack --config webpack.dev.js",
    "build-ssr": "webpack --config webpack.ssr.js && node scripts/ssrScript.js",
    "build-prod-no-test": "npm-run-all clean 'webpack --config webpack.prod.js'",
    "build-prod": "npm-run-all clean test 'webpack --config webpack.prod.js'",
    "build-github-action": "npm-run-all clean 'webpack --config webpack.prod.js --env ci=github'",
    "webpack": "webpack",
    "webpack-dev-server": "webpack serve --config webpack.dev.js --port 9211",
    "devrun": "NODE_ENV=DEV npm-run-all clean webpack-dev-server",
    "test": "NODE_ENV=test echo 'Running JS tests' && jest",
    "test-watch": "NODE_ENV=test echo 'Running JS tests in watch mode' && jest --watch",
    "jest-update-snapshot": "echo 'Updating JS tests' && jest -u",
    "paparazzi": "paparazzi && open ./paparazzi",
    "prepare": "cd .. && husky install ./support-frontend/.husky",
    "prettier:check": "prettier --check ./assets",
    "prettier:fix": "prettier --write ./assets",
    "storybook": "storybook dev -p 9001 --no-open",
    "build-storybook": "storybook build -o storybook-static",
    "cache-ssr": "node scripts/ssrScript.js",
    "orphans": "echo 'Checking for orphan modules' && node scripts/orphans.js",
    "check-images": "echo 'Purging unused images' && node scripts/gridImages.js"
  },
  "browserslist": [
    "extends @guardian/browserslist-config"
  ],
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)",
      "!**/__fixtures__/**"
    ],
    "transform": {
      ".+\\.(js|jsx|ts|tsx)$": "./node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!@guardian|valibot)",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "es6",
      "jsx",
      "tsx"
    ],
    "modulePaths": [
      "assets"
    ],
    "modulePathIgnorePatterns": [
      "target",
      "assets copy"
    ],
    "moduleNameMapper": {
      "ophan(.*)": "<rootDir>/node_modules/ophan-tracker-js/build/ophan.support",
      "\\.(css|less|sass|scss)$": "<rootDir>/assets/__mocks__/styleMock.ts",
      "\\.(svg|png|jpg)$": "<rootDir>/assets/__mocks__/imageMock.ts"
    },
    "setupFilesAfterEnv": [
      "./jestSetup"
    ],
    "verbose": true,
    "testEnvironment": "./jestEnvironment"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guardian/support-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/guardian/support-frontend/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.7",
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.11.1",
    "@emotion/serialize": "^1.1.2",
    "@emotion/utils": "^1.1.0",
    "@floating-ui/react": "^0.19.2",
    "@guardian/libs": "16.1.0",
    "@guardian/pasteup": "1.0.0-alpha.7",
    "@guardian/source-foundations": "^13.0.0",
    "@guardian/source-react-components": "^16.0.1",
    "@guardian/source-react-components-development-kitchen": "^14.1.0",
    "@guardian/tsconfig": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.4",
    "@sentry/browser": "^7.72.0",
    "@stripe/react-stripe-js": "^1.8.1",
    "@stripe/stripe-js": "^1.54.2",
    "@types/dompurify": "^3.0.2",
    "@types/seedrandom": "^3.0.8",
    "@types/uuid": "^9.0.2",
    "classnames": "^2.2.6",
    "dompurify": "^3.0.3",
    "emotion-theming": "^11.0.0",
    "framer-motion": "^1.11.1",
    "lodash.debounce": "^4.0.6",
    "mockdate": "^3.0.5",
    "ophan-tracker-js": "^1.4.3",
    "react-day-picker": "^7.4.8",
    "react-is": "^18.2.0",
    "react-redux": "^8.1.0",
    "react-router-dom": "^6.23.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "seedrandom": "^3.0.5",
    "snarkdown": "^2.0.0",
    "utility-types": "^3.10.0",
    "uuid": "^9.0.0",
    "valibot": "^0.28.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/react": "^4.2.1",
    "@babel/core": "^7.21.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-exponentiation-operator": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/eslint-plugin": "^11.11.0",
    "@guardian/browserslist-config": "^5.0.0",
    "@guardian/eslint-config-typescript": "^7.0.0",
    "@guardian/eslint-plugin-source-foundations": "^15.0.0",
    "@guardian/eslint-plugin-source-react-components": "^19.0.0",
    "@guardian/paparazzi": "^0.3.1",
    "@guardian/prettier": "^2.1.5",
    "@khanacademy/flow-to-ts": "^0.5.1",
    "@storybook/addon-a11y": "^7.4.2",
    "@storybook/addon-actions": "^7.6.10",
    "@storybook/addon-essentials": "^7.4.3",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-storysource": "^7.6.12",
    "@storybook/addon-viewport": "^7.6.10",
    "@storybook/jest": "^0.2.1",
    "@storybook/preact": "^8.0.8",
    "@storybook/preact-webpack5": "^7.3.2",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
