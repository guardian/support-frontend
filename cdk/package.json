{
  "name": "cdk",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "private": true,
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:dev": "jest --watch",
    "format": "prettier --write \"{lib,bin}/**/*.ts\"",
    "check-formatting": "prettier --check \"{lib,bin}/**/*.ts\"",
    "cdk": "cdk",
    "lint": "eslint lib/** bin/** --ext .ts --no-error-on-unmatched-pattern",
    "synth": "cdk synth --path-metadata false --version-reporting false",
    "diff": "cdk diff --path-metadata false --version-reporting false"
  },
  "devDependencies": {
    "@guardian/cdk": "61.8.1",
    "@guardian/eslint-config-typescript": "catalog:",
    "@guardian/prettier": "4.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "24.5.2",
    "aws-cdk": "2.1007.0",
    "aws-cdk-lib": "2.189.1",
    "constructs": "10.4.2",
    "eslint": "8.57.1",
    "jest": "29.7.0",
    "prettier": "2.8.8",
    "source-map-support": "0.5.21",
    "ts-jest": "29.3.4",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.3.3"
  }
}
